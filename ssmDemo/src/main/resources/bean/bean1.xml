<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="car1" class="bean.Car"/>
    <bean id="car2" class="bean.Car"/>
    <bean id="car3" class="bean.Car"/>
<bean id="boss1" class="bean.Boss">
    <property name="name" value="张三"/>
    <property name="hobby" value="羽毛球"/>
    <property name="company" value="公司a"/>
    <property name="car" ref="car1"/>
</bean>
    <bean id="boss2" class="bean.Boss">
        <property name="name" value="李强"/>
        <property name="hobby" value="篮球"/>
        <property name="company" value="公司b"/>
        <property name="car" ref="car2"/>
    </bean>
    <bean id="boss3" class="bean.Boss">
        <property name="name" value="刘名"/>
        <property name="hobby" value="打牌"/>
        <property name="company" value="公司c"/>
        <property name="car" ref="car3"/>
    </bean>
</beans>